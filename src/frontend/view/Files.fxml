<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Tooltip?>
<fx:root minHeight="100.0" prefHeight="900.0" prefWidth="250.0" styleClass="leftSideMenu" stylesheets="@css/DarkMode.css" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label prefWidth="230.0" styleClass="leftSideMenu-textLarge" stylesheets="@css/DarkMode.css" text="Load Diagram" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
      <VBox fx:id="filesBox" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0" />
      <Button fx:id="save_button" layoutX="23.0" layoutY="22.0" mnemonicParsing="false" onMouseClicked="#openSave" styleClass="buttons" stylesheets="@css/DarkMode.css" text="SAVE" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="22.0">
         <tooltip>
            <Tooltip text="Save changes" />
         </tooltip></Button>
      <Button fx:id="delete_button" layoutX="20.0" layoutY="76.0" mnemonicParsing="false" styleClass="buttons" stylesheets="@css/DarkMode.css" text="DELETE DIAGRAM">
         <tooltip>
            <Tooltip text="Delete the diagram" />
         </tooltip></Button>
      <Button mnemonicParsing="false" onAction="#newFile" prefHeight="30.0" prefWidth="200.0" styleClass="highlight-green" stylesheets="@css/DarkMode.css" text="New Diagram" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
   </children>
</fx:root>
